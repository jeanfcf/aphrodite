<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="telaHistorico" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controlador">
   <children>
      <ImageView fitHeight="592.0" fitWidth="629.0" layoutX="1.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Aphrodite.png" />
         </image>
      </ImageView>
      <Button fx:id="btnVoltar" layoutX="465.0" layoutY="33.0" mnemonicParsing="false" onAction="#aBtVoltar" prefHeight="51.0" prefWidth="110.0" text="Voltar" />
      <ScrollPane fx:id="scrollPaneHistorico" layoutX="39.0" layoutY="107.0">
         <content>
            <ListView fx:id="listHistorico" prefHeight="479.0" prefWidth="534.0" />
         </content>
      </ScrollPane>
      <Button fx:id="btnAtualizar" layoutX="39.0" layoutY="33.0" mnemonicParsing="false" onAction="#aBtAtualizar" prefHeight="51.0" prefWidth="110.0" text="Atualizar" />
      <Label fx:id="lblSaldo" layoutX="179.0" layoutY="33.0" prefHeight="51.0" prefWidth="272.0" text="Saldo:">
         <font>
            <Font name="Candara" size="31.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
