<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="telaAdicionarTipo" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controlador">
   <children>
      <ImageView fitHeight="592.0" fitWidth="629.0" layoutX="1.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Aphrodite.png" />
         </image>
      </ImageView>
      <Button fx:id="btnAdicionarTipoConfirma" layoutX="462.0" layoutY="532.0" mnemonicParsing="false" onAction="#aBtAdicionarTipoConfirma" prefHeight="35.0" prefWidth="100.0" text="Adicionar" />
      <Button fx:id="btnCancelarAdicionarTipo" layoutX="62.0" layoutY="532.0" mnemonicParsing="false" onAction="#aBtCancelarAdicionarTipo" prefHeight="35.0" prefWidth="100.0" text="Cancelar" />
      <TextField fx:id="txtAdicionar" layoutX="225.0" layoutY="110.0" prefHeight="35.0" prefWidth="171.0" promptText="oi amor te amo" />
      <MenuButton fx:id="menuEscolha" layoutX="71.0" layoutY="115.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="113.0" text="Escolha ">
        <items>
          <MenuItem fx:id="menuCor" mnemonicParsing="false" onAction="#aMcor" text="COR" />
          <MenuItem fx:id="menuTamanho" mnemonicParsing="false" onAction="#aMtamanho" text="TAMANHO" />
            <MenuItem fx:id="menuModelo" mnemonicParsing="false" onAction="#aMmodelo" text="MODELO" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
